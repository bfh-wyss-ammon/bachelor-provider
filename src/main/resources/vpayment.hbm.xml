<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="data.DbVPayment" table="payments">
		<composite-id>
			<key-property name="groupId" type="int" column="groupid" />
			<key-property name="period" column="period" />
		</composite-id>
		<property name="total">
			<column name="total" />
		</property>
	</class>
</hibernate-mapping>
